        }
    ],
    "require": {
        "symfony/console": "4.1.*",
        "symfony/stopwatch": "4.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.3.*",
        "phpspec/phpspec": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\App\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "test": [
            "bin/phpspec run --ansi"
        ]
    }
}
